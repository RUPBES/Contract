            <td rowspan="4">
                #-?
            </td>
            <td rowspan="4">
                Процедура выбора -
                @foreach (var item in Model.SelectionProcedures)
                {
                    @item.Name
                }
            </td>
            <td rowspan="4">
                Подписанты-
                @foreach (var item in Model.EmployeeContracts)
                {
                    if (@item.IsSignatory == true)
                    {
                        @item.Employee.FullName
                    }
                    else
                    if (@item.IsResponsible == true)
                    {
                        @item.Employee.FullName
                    }
                }
                <br>
                Model.ResponsibleEmp
            </td>
            <td rowspan="4">
                Номер и дата-
                @Model.Number от

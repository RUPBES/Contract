@model MvcLayer.Models.ContractViewModel

@{
    ViewData["Title"] = "Details";
}
<div class="full-width">
    <table class="table table-main table-main-full table-striped table-bordered" id="contractTable" >
        <thead>
            <tr class="tr-head-main-table">
                @*№*@
                <th rowspan="2">
                    <span>#</span>
                </th>
                @*Процедура выбора*@
                <th rowspan="2">
                    <span>Процедура выбора</span>
                </th>
                @*Отвественное лицо*@
                <th rowspan="2">
                    <span>Подписант договора, лицо ответственное за ведение договора</span>                    
                </th>
                @*Номер и дата договора*@
                <th rowspan="2">
                    <span>Номер и дата договора</span>                    
                </th>
                @*Наименование объекта*@
                <th rowspan="2">
                    <span class="table_span-nameobject">Наименование объекта</span>
                </th>
                @*Источник финансирования*@
                <th rowspan="2">
                    <span>Источник финансирования</span>                    
                </th>                
                @*Заказчик*@
                <th rowspan="2">
                    <span>Заказчик</span>                    
                </th>
                @*Генподрядчик*@
                <th rowspan="2">
                    <span>Генподрядчик (филиал-испольнитель по приказу)</span>                  
                </th>
                @*Субподрядчик (филиал-исполнитель по приказу)*@
                <th rowspan="2">
                    <span>Субподрядчик (филиал-исполнитель по приказу)</span>                    
                </th>
                @*Cроки*@
                <th rowspan="2">
                    <span>Cроки</span>                    
                </th>
                @*Акты приостановки и возобновления работ*@
                <th rowspan="2">
                    <span>Акты приостановки и возобновления работ</span>                    
                </th>
                @*Условия оплаты*@
                <th rowspan="2">
                    <span>Условия оплаты, без авансов тек. % цел.% отсрочка/в течение</span>                   
                </th>
                @*Услуги генподряда (для договоров субподряда)*@
                <th rowspan="2">
                    <span>Услуги генподряда (для договоров субподряда)</span>                    
                </th>
                @*Материалы по факту или сметной стоимости согласно договору*@
                <th rowspan="2">
                    <span class='table_span-material'>Материалы по факту или сметной стоимости согласно договору</span>
                </th>
                @*Валюта расчетов*@
                <th rowspan="2">
                    <span class="table_span-currency">Валюта расчетов</span>
                </th>
                @*Виды работ по договору*@
                <th rowspan="2">
                    <span>Виды работ по договору</span>                    
                </th>
                @*Контрактная договорная цена*@
                <th rowspan="2">
                    <span class="table_span-contractprice">Контрактная договорная цена, с НДС</span>
                </th>
                @*Выполнено на 01.01 текущего года*@
                <th rowspan="2">
                    <span class="table_span-realization">Выполнено на 01.01 текущего года</span>                    
                </th>
                @*Остаток по выполнению от стоимости контрактной цены*@
                <th rowspan="2">
                    <span class="table_span-remains">Остаток по выполнению от стоимости контрактной цены</span>
                </th>
                @*Объем на текущий год*@
                <th rowspan="2">
                    <span class="table_span-volume">Объем на текущий год</span>
                </th>
                @*В том числе:*@
                <th colspan="12">
                    <span>В том числе:</span>                    
                </th>
                @*Акт ввода*@
                <th rowspan="2">
                    <span>Акт ввода</span>                    
                </th>
                @*Переписка*@
                <th rowspan="2">
                    <span>Переписка</span>                    
                </th>
                @*Проектно-сметная документация*@
                <th rowspan="2">
                    <span>Проектно-сметная документация</span>                    
                </th>
            </tr>
            @*Месяцы*@
            <tr class="tr-head-main-table">
                <th><span class="table_span-month">январь</span></th>
                <th><span class="table_span-month">февраль</span></th>
                <th><span class="table_span-month">март</span></th>
                <th><span class="table_span-month">апрель</span></th>

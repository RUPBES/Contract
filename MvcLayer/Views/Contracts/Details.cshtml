            <tr id="firstrow">
                <td rowspan="4">
                    #-?
                </td>
                <td rowspan="4">
                    Процедура выбора -
                    @foreach (var item in Model.SelectionProcedures)
                    {
                        @item.Name
                    }
                </td>
                <td rowspan="4">
                    Подписанты-
                    @foreach (var item in Model.EmployeeContracts)
